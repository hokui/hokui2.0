<h1>学期一覧</h1>

<table class="term_table">
  <tr>
    <th>学期コード</th>
    <th>学期名</th>
    <th>時間割表のMIMEタイプ</th>
    <th>時間割表</th>
  </tr>
  <% @terms.each do |term| %>
    <tr>
      <td><%= term.term_identifier %></td>
      <td><%= term.term_name %></td>
      <td><%= term.term_timetable_img_content_type %></td>
      <td></td>
    </tr>
  <% end %>
</table>
